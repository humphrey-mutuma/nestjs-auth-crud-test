      
model Product {
  id Int @default(autoincrement()) @id
  name String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  price Float
  sale Boolean @default(false)
  availibility Availibility
  reviews Review[]
  tags Tag[]
  description String?
}

enum Availibility {
  IN_STORE
  ONLINE
}